{
  "name": "bot-busters",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/ && rimraf .next/",
    "copy-files": "copyfiles -u 1 src/**/*.gif src/**/*.png dist/",
    "build:next": "next build",
    "build:ws": "yarn copy-files && tsc --project tsconfig.server.json && tsc-alias --project tsconfig.server.json",
    "build": "yarn build:next && yarn build:ws",
    "start:next": "next start",
    "test": "dotenv -e .env.test jest",
    "test:db": "dotenv -e .env.test drizzle-kit push:pg",
    "start:ws": "dotenv node dist/server/api/ws-server.js",
    "start:prod": "dotenv node dist/server/api/prod-server.js",
    "start": "concurrently \"yarn start:next\" \"yarn start:ws\"",
    "db:run": "docker-compose up -d",
    "db:push": "dotenv drizzle-kit push:pg",
    "db:studio": "dotenv drizzle-kit studio",
    "dev": "next dev",
    "dev:ws": "yarn build:ws && yarn start:ws",
    "lint": "next lint"
  },
  "dependencies": {
    "@aleohq/sdk": "^0.6.2",
    "@demox-labs/aleo-wallet-adapter-base": "^0.0.19",
    "@demox-labs/aleo-wallet-adapter-leo": "^0.0.18",
    "@demox-labs/aleo-wallet-adapter-react": "^0.0.18",
    "@demox-labs/aleo-wallet-adapter-reactui": "^0.0.30",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.11",
    "@t3-oss/env-nextjs": "^0.7.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "drizzle-orm": "^0.28.6",
    "drizzle-zod": "^0.5.1",
    "lodash": "^4.17.21",
    "minidenticons": "^4.2.0",
    "next": "13.4.12",
    "next-auth": "^4.23.0",
    "postgres": "^3.3.5",
    "puppeteer": "^21.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.6",
    "superjson": "^1.13.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/stats": "^0.16.30",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "leva": "^0.9.35",
    "pg": "^8.11.3",
    "react-fps": "^1.0.6",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.20.2"
  },
  "packageManager": "yarn@3.6.3"
}
